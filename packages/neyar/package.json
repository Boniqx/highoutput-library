    {
      "path": "dist/editor.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/code": "^2.7.0",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.186",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "swcify": "^1.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.24.3",
