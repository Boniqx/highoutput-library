{
  "name": "arque",
  "version": "1.0.0-alpha.24",
  "description": "A library for Event Sourcing",
  "keywords": [
    "microservice",
    "event sourcing"
  ],
  "author": "High Output Ventures",
  "homepage": "https://github.com/Proto-Garage/highoutput-library/tree/master/packages/arque#README.md",
  "license": "ISC",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HighOutputVentures/highoutput-library.git"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "clean": "rimraf build/",
    "build": "npm run clean && tsc --project tsconfig.json",
    "prepublish": "npm run build",
    "examples:basic": "cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true ts-node src/examples/basic/test.ts && echo 'Success'",
    "examples:distributed": "cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true ts-node src/examples/distributed/test.ts && echo 'Success'",
    "test": "npm run examples:basic && npm run examples:distributed"
  },
  "bugs": {
    "url": "https://github.com/HighOutputVentures/highoutput-library/issues"
  },
  "dependencies": {
    "@arque/core": "^1.0.0-alpha.5",
    "@arque/types": "^1.0.0-alpha.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.9.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "f378c3ee861ff2ece44b93f061bc64510f9e0427"
}
