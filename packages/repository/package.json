{
  "name": "@highoutput/repository",
  "version": "0.1.0-rc.3",
  "description": "Repository Class Implementation",
  "author": "High Output Ventures",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HighOutputVentures/highoutput-library.git"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "prebuild": "rimraf dist",
    "build": "tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 5000000,
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "verbose": true,
    "maxWorkers": "75%"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@highoutput/object-id": "^0.1.6",
    "mongoose": "~6.3.4",
    "ramda": "~0.28.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/ramda": "^0.28.13"
  }
}
